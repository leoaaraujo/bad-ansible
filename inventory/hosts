[frontends] # These are the frontend servers
frontend1 ansible_host=frontend1.e33a.internal

[apps] ## These are the apps servers
app1 ansible_host=app1.e33a.internal
app2 ansible_host=app2.e33a.internal

[appdbs] # These are the appdb servers
appdb1 ansible_host=appdb1.e33a.internal

[internal:children]
frontends
apps
appdbs
